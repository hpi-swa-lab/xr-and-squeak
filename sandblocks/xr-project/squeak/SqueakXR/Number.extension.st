Extension { #name : #Number }

{ #category : #'*SqueakXR' }
Number >> cm [

	^ self * 0.01
]

{ #category : #'*SqueakXR' }
Number >> dm [

	^ self * 0.1
]

{ #category : #'*SqueakXR' }
Number >> km [

	^ self * 1000
]

{ #category : #'*SqueakXR' }
Number >> m [

	^ self
]

{ #category : #'*SqueakXR' }
Number >> mm [

	^ self * 0.001
]
